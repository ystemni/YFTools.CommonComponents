<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;net45</TargetFrameworks>
		<LangVersion>10.0</LangVersion>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageId>YFTools.CommonComponents</PackageId>
		<Authors>yangfei</Authors>
		<Product>YFTools.CommonComponents</Product>
		<Copyright>Copyright (c) 2020-2022 YangFei. All rights reserved.</Copyright>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags>CommonComponents</PackageTags>
		<AnalysisLevel>none</AnalysisLevel>
		<PlatformTarget>AnyCPU</PlatformTarget>
		<Version>1.0.0</Version>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Description>通用组件</Description>
	</PropertyGroup>

	<ItemGroup>
	  <Folder Include="Exceptions\" />
	  <Folder Include="Extensions\" />
	  <Folder Include="Helpers\" />
	  <Folder Include="Models\" />
	  <None Include="docs\README.md">
		  <Pack>True</Pack>
		  <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>
